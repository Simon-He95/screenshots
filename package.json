{
  "name": "root",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "packageManager": "pnpm@7.33.7",
  "scripts": {
    "preinstall": "node ./scripts/enforce-pnpm.js",
    "bootstrap": "pnpm install",
    "dev": "pnpm -r run dev --parallel",
    "start": "pnpm -r run start --parallel",
    "build": "pnpm -r run build",
    "clean": "pnpm -r run clean",
    "release": "pnpm publish -r --no-git-checks",
    "alpha": "pnpm publish -r --tag alpha"
  }
}
